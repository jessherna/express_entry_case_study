	measure 'Invitations PY Gap with Arrow' =
			
			VAR uparrow = UNICHAR(129129)
			VAR downarrow = UNICHAR(129131)
			VAR invitationgap = IF(ISBLANK([Invitations PY Gap]), 0, [Invitations PY Gap])
			
			RETURN
			SWITCH(
			    TRUE(),
			    invitationgap = 0, "--",
			    invitationgap < 0, FORMAT(invitationgap, "##,###") & " " & downarrow,
			    invitationgap > 0, FORMAT(invitationgap, "##,###") & " " & uparrow
			)
